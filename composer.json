{
    "name": "mikamatto/breadcrumbs-bundle",
    "description": "A Symfony bundle for managing breadcrumbs.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikamatto",
            "email": "mick@mikamatto.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^5.0|^6.0|^7.0",
        "symfony/yaml": "^5.0|^6.0|^7.0",
        "symfony/routing": "^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mikamatto\\BreadcrumbsBundle\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    },
    "minimum-stability": "stable",
    "keywords": ["breadcrumbs", "Symfony", "navigation", "bundle", "twig"]
}